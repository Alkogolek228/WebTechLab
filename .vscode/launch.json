{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Launch Web_253502_Alkhovik",
        "type": "coreclr",
        "request": "launch",
        "preLaunchTask": "build",
        "program": "${workspaceFolder}/Web_253502_Alkhovik/bin/Debug/net8.0/Web_253502_Alkhovik.dll",
        "args": [],
        "cwd": "${workspaceFolder}/Web_253502_Alkhovik",
        "stopAtEntry": false,
        "serverReadyAction": {
          "action": "openExternally",
          "pattern": "\\bNow listening on:\\s+(https?://\\S+)"
        },
        "env": {
          "ASPNETCORE_ENVIRONMENT": "Development"
        },
        "sourceFileMap": {
          "/Views": "${workspaceFolder}/Web_253502_Alkhovik/Views"
        }
      },
      {
        "name": "Launch Web_253502_Alkhovik.API",
        "type": "coreclr",
        "request": "launch",
        "preLaunchTask": "build",
        "program": "${workspaceFolder}/Web_253502_Alkhovik.API/bin/Debug/net8.0/Web_253502_Alkhovik.API.dll",
        "args": [],
        "cwd": "${workspaceFolder}/Web_253502_Alkhovik.API",
        "stopAtEntry": false,
        "serverReadyAction": {
          "action": "openExternally",
          "pattern": "\\bNow listening on:\\s+(https?://\\S+)"
        },
        "env": {
          "ASPNETCORE_ENVIRONMENT": "Development"
        },
        "sourceFileMap": {
          "/Views": "${workspaceFolder}/Web_253502_Alkhovik.API/Views"
        }
      }
    ],
    "compounds": [
      {
        "name": "Launch Both Projects",
        "configurations": ["Launch Web_253502_Alkhovik", "Launch Web_253502_Alkhovik.API"]
      }
    ]
  }